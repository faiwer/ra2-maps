{
  "name": "ra2-maps",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsgo --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsgo --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "prepare": "husky",
    "deploy": "./deploy.sh",
    "add-map": "./addMap.sh"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,scss}": "prettier --write"
  },
  "dependencies": {
    "antd": "^6.1.4",
    "react": "npm:@faiwer/react@^19.0.4",
    "react-dom": "npm:@faiwer/react-dom@^19.0.1"
  },
  "devDependencies": {
    "@typescript/native-preview": "^7.0.0-dev",
    "@vitejs/plugin-react": "^4.3.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "sass": "^1.83.0",
    "vite": "^6.0.7"
  },
  "author": "",
  "license": "ISC"
}
